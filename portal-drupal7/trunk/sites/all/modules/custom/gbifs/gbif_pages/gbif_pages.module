/**
 * Implements hook_menu_alter().
 */
function gbif_pages_menu_alter(&$items) {
  $items['taxonomy/term/%taxonomy_term']['page callback'] = 'gbif_pages_term_page';
}

function gbif_pages_term_page($term) {
  $build = taxonomy_term_page($term);
  return $build;
}